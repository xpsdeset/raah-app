{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "b:a": "expo build:android -t app-bundle --skip-workflow-check",
    "b:i": "expo build:ios -t archive --skip-workflow-check",
    "u:a": "expo upload:android",
    "u:i": "expo upload:ios",
    "predeploy": "expo build:web",
    "deploy": "yarn predeploy && firebase deploy --only hosting"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "expo": "^41.0.0",
    "expo-cli": "^4.4.1",
    "expo-constants": "~10.1.3",
    "expo-document-picker": "~9.1.2",
    "expo-firebase-recaptcha": "^1.4.1",
    "expo-image-picker": "~10.1.3",
    "expo-notifications": "~0.11.5",
    "expo-permissions": "~12.0.1",
    "expo-status-bar": "~1.0.4",
    "firebase": "8.2.3",
    "global": "^4.4.0",
    "lodash": "^4.17.15",
    "modal-react-native-web": "^0.2.0",
    "moment": "^2.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.4",
    "react-native-elements": "^3.3.2",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-reanimated": "~2.10.0",
    "react-native-root-siblings": "^4.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-unimodules": "~0.13.3",
    "react-native-web": "~0.13.12",
    "react-native-web-webview": "^1.0.2",
    "react-native-webview": "11.2.3",
    "react-redux": "^7.2.0",
    "react-redux-firebase": "^3.6.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.js --fix-dry-run"
    ]
  },
  "private": true
}
